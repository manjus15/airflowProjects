    <connection>SEC_SOURCE</connection>
    <sql>select a.mr_no,a.patient_name,a.middle_name,a.last_name,a.expected_dob,a.dateofbirth,a.patient_gender,
    a.custom_list1_value,a.passport_no,a.custom_field2
FROM  insurance_submission_batch isb
LEFT JOIN insurance_claim ic on(ic.submission_batch_id=isb.submission_batch_id)
JOIN patient_registration pr on ic.patient_id=pr.patient_id
JOIN  patient_details a on a.mr_no = pr.mr_no
JOIN hospital_center_master hcm on hcm.center_id=pr.center_id
WHERE hcm.center_id in (${CenterID}) and isb.submission_date::date>= ${FromDate} AND isb.submission_date::date&lt;= ${ToDate}
</sql>
